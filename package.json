{
    "name": "highly-divisible-triangular-number",
    "version": "1.0.0",
    "description": "A program to find highly divisible triangular numbers",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "build": "tsc",
        "test": "jest",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "author": "Aditya Viki",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.24",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.5.1",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
